<gama-local>
<network axes-xy="sw" angles="right-handed">
<description>
	vodorovna delka
	zenitovy uhel
</description>
<parameters sigma-apr="1.00" 
	conf-pr="0.95" 
	sigma-act="aposteriori" 
	update-constrained-coordinates="no"/>

<points-observations direction-stdev="10" 
	distance-stdev="2 2" 
	zenith-angle-stdev="10">

<point id="1" adj="xyz"/>
<point id="2" adj="xyz"/>

<obs from="1">
	<distance to="2" val="100.1"/>
	<z-angle   to="2" val="50"/>
</obs>

<coordinates>
	<point id="1" x="5000" y="1000" z="0"/>
	<point id="2" x="5000" y="1100" z="100"/>
	<cov-mat dim="6" band="0">
		500 500 500 500 500 500
	</cov-mat>
</coordinates>

</points-observations>
</network>
</gama-local>
